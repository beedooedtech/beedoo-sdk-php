{
    "name": "beedooedtech/beedoo-sdk-php",
    "description": "Beedoo API PHP SDK",
    "type": "library",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "license": "MIT",
    "keywords": [
        "beedoo.com.br",
        "beedoo",
        "edtech",
        "api"
    ],
    "scripts": {
        "test": [
            "phpunit --coverage-html reports"
        ]
    },
    "autoload": {
        "psr-4": {
            "Beedoo\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BeedooTests\\": "tests/",
            "Tools\\": "tools/"
        }
    },
    "authors": [
        {
            "name": "Igor Santana",
            "email": "igor.santana@beedoo.com.br"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "guzzlehttp/guzzle": "^6.5.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    }
}
